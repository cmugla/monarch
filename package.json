{
  "name": "@wework/monarch",
  "version": "1.0.0-alpha.12",
  "description": "Tool for dynamically generating Contentful Migrations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wework/monarch.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/wework/monarch",
  "bin": {
    "monarch": "./bin/monarch"
  },
  "scripts": {
    "test": "jest",
    "parse:raw": "node bin/monarch parse:raw",
    "parse:react": "node bin/monarch parse:react"
  },
  "dependencies": {
    "contentful-migrate": "^0.8.0",
    "fs": "^0.0.1-security",
    "lodash": "4.17.11",
    "react": "16.5.2",
    "react-docgen": "2.21.0",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "husky": "1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "8.0.4",
    "prettier": "1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
